var fs = require("fs");
fs.readFileSync(process.argv[2]).toString().split('\n').forEach(function (line) {
    if (line != "") {
        line = line.split(" ");
        var integerInteger = line.length;
        var integer = line[integerInteger - 1];
        integer = parseInt(integer, 10);

        if (integer < line.length) {
            var output = line[integerInteger - integer - 1];
            console.log(output);
        } else;
    }
});
